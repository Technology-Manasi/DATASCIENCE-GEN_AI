   "source": [
    "def runningSum(nums):\n",
    "    result = []\n",
    "    current_sum = 0\n",
    "\n",
    "    for n in nums:\n",
    "        current_sum += n\n",
    "        result.append(current_sum)\n",
    "\n",
    "    return result\n"
   ]
